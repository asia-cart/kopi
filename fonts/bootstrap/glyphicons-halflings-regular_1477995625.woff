    <script type="text/javascript">
        $(function() {
            $('#sq').on('keypress', function(e) {
                // $('.wrap-search .list-search').empty();
                if (e.which == 13 && $(this).val().length >= 3) {
                    $('.wrap-search .list-search').empty();
                    var post_search = [
                        {
                            name: 'sq',
                            value: $(this).val()
                        }
                    ];
                    ajax_post('http://kopitop.com/search', post_search)
                    .done(function(response) {
                        if (response['output']) {
                            $('.wrap-search .list-search').html(response['output']);
                        }
                    })
                }
            })
        })
    </script>


